{
  "name": "uptrend-website",
  "private": true,
  "description": "Uptrend Website",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "lint": "kcd-scripts lint",
    "format": "kcd-scripts format",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "dependencies": {
    "@emotion/core": "^10.0.16",
    "@mdx-js/react": "^1.4.0",
    "gatsby": "^2.13.73",
    "gatsby-image": "^2.2.19",
    "gatsby-plugin-sharp": "^2.2.22",
    "gatsby-plugin-theme-ui": "^0.2.38",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-transformer-sharp": "^2.2.14",
    "is-absolute-url": "^3.0.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "theme-ui": "^0.2.38",
    "typeface-comfortaa": "^0.0.76"
  },
  "devDependencies": {
    "kcd-scripts": "^1.7.0",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/jest"
    ],
    "rules": {
      "no-process-exit": "off",
      "import/no-dynamic-require": "off",
      "import/no-unassigned-import": "off",
      "no-console": "off",
      "no-nested-ternary": "off",
      "no-useless-catch": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uptrend-tech/uptrend-website"
  },
  "bugs": {
    "url": "https://github.com/uptrend-tech/uptrend-website/issues"
  }
}
